<?xml version="1.0" encoding="UTF-8" standalone="no"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:firstElementOfTheInput="root" xmlns:notXSLTCompatible="false" xmlns:own="http://ownfunction.org" xmlns:xs="http://www.w3.org/2001/XMLSchema" extension-element-prefixes="xs own notXSLTCompatible firstElementOfTheInput" version="2.0"><xsl:function name="own:setPrecision"><xsl:param name="resultString"/><xsl:param name="noOfDigits"/><xsl:if test="$noOfDigits=0"><xsl:value-of select="$resultString"/></xsl:if><xsl:if test="$noOfDigits!=0"><xsl:value-of select="own:setPrecision(concat($resultString,'0'),$noOfDigits - 1)"/></xsl:if></xsl:function><xsl:template match="/"><root><invoiceId><xsl:value-of select="number( root/invoiceId )"/></invoiceId><invoiceAmt><xsl:value-of select="root/invoiceAmt"/></invoiceAmt><currency><xsl:value-of select="root/currency"/></currency><creditPayment><xsl:value-of select="root/creditPayment eq 'true' "/></creditPayment><onlinePayment><xsl:value-of select="root/onlinePayment eq 'true' "/></onlinePayment><cashPayment><xsl:value-of select="root/cashPayment eq 'true' "/></cashPayment><address_line><xsl:value-of select="root/address_line"/></address_line><address_city><xsl:value-of select="root/address_city"/></address_city><address_state><xsl:value-of select="root/address_state"/></address_state><address_pincode><xsl:value-of select="root/address_pincode"/></address_pincode><whenCreated><xsl:value-of select="substring( root/whenCreated , number('0') , number('0') + number('10') )"/></whenCreated></root></xsl:template></xsl:stylesheet>